<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <title>@ViewBag.Title</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link href="~/Content/js/layui/css/layui.css" rel="stylesheet" />
    <script src="~/Content/js/layui/layui.all.js"></script>
    <script type="text/javascript" src="~/Content/js/tableSelect.js"></script>
   
    <script>
        layui.config({
            dir: '~/Content/js/layui/' //layui.js 所在路径（注意，如果是script单独引入layui.js，无需设定该参数。），一般情况下可以无视
            , version: true //一般用于更新模块缓存，默认不开启。设为true即让浏览器不缓存。也可以设为一个固定的值，如：201610
            , debug: false //用于开启调试模式，默认false，如果设为true，则JS模块的节点会保留在页面
            , base: '/Content/js/layui/plugin/' //设定扩展的Layui模块的所在目录，一般用于外部模块扩展
        });
        var host = "/api/BenDing";
        var hostPage = "/Insurance";
        var hostNew = "/api/BenDingNew";
    </script>
</head>
<body>
    <!-- 让IE8/9支持媒体查询，从而兼容栅格 -->
    <!--[if lt IE 9]>
      <script src="~/Content/js/html5/html5shiv.min.js"></script>
      <script src="~/Content/js/html5/respond.min.js"></script>
    <![endif]-->
    <script src="~/Content/js/html5/html5shiv.min.js"></script>
    <script src="~/Content/js/html5/respond.min.js"></script>
    @RenderBody()
    @Html.AntiForgeryToken()
</body>
</html>
